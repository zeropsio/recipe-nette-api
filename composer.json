{
  "name": "zeropsio/recipe-nette-api",
  "description": "Zerops demo Nette ",
  "keywords": [
    "zerops",
    "zeropsio",
    "nette"
  ],
  "type": "project",
  "license": [
    "MIT",
    "BSD-3-Clause",
    "GPL-2.0",
    "GPL-3.0"
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "ext-mbstring": "*",
    "nette/application": "^3.1",
    "nette/bootstrap": "^3.1",
    "nette/database": "^3.1",
    "nette/di": "^3.0",
    "nette/finder": "^2.5",
    "nette/http": "^3.1",
    "nette/robot-loader": "^3.3",
    "tracy/tracy": "^2.8",
    "contributte/api-router": "^4.2",
    "nettrine/orm": "^0.8.3",
    "nettrine/dbal": "^0.8.0",
    "nettrine/cache": "^0.3.0",
    "contributte/console": "^0.9.2",
    "nettrine/migrations": "^0.8.0",
    "nettrine/fixtures": "^0.6.2",
    "contributte/middlewares": "^0.10.2",
    "tuupola/cors-middleware": "^1.3",
    "contributte/monolog": "^0.5.0"
  },
  "require-dev": {
    "nette/tester": "^2.3",
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-doctrine": "^1.3",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-nette": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "symfony/thanks": false,
      "phpstan/extension-installer": true
    }
  }
}
